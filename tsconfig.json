{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "strict": true,
    "paths": {
      "@shared/*": ["./src/shared/*"],
      "@features/*": ["./src/features/*"],
      "@core/*": ["./src/core/*"],
      "@router/*": ["./src/router/*"],
      "@assets/*": ["./src/assets/*"],
  },
  }
}
